            @Html.DisplayFor(modelItem => item.JobID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.JobSeekerID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.JobAppliedDate)
